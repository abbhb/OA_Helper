<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qc.printers.mapper.StudyClockMapper">
    <!--    public List<StudyClock> getStudyClock(@Param("id") Long id,@Param("startTime") LocalDateTime startTime,@Param("endTime") LocalDateTime endTime);
    -->
    <select id="getStudyClock" resultType="com.qc.printers.pojo.StudyClock">
        SELECT *
        FROM study_clock
        WHERE user_id = #{id}
          and date between #{startTime}
          and #{endTime}

    </select>


</mapper>